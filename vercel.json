{
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.theclicks.ca" }],
      "destination": "https://theclicks.ca/:path*",
      "permanent": true
    },
    {
      "source": "/:path*/",
      "destination": "/:path*",
      "permanent": true
    }
  ]
}
